    <add key="FormatStartMark" value="&lt;f=|"/>      --><!--<f=|--><!--
    ...
  </default_setting>
  
  <new_setting>
    <add key="KeyValueSplitMark" value="&lt;===&gt;"/>  --><!--<===>--><!--
