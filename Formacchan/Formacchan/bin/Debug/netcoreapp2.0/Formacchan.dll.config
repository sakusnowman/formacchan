<app_setting table="default_setting">
  <default_setting>
    <add key="isXmlMode" value="true"/>
    <add key="KeyValueSplitMark" value="&lt;=&gt;"/>  <!--<=>-->
    <add key="FormatStartMark" value="&lt;f=|"/>      <!--<f=|-->
    <add key="FormatEndMark" value="|=f&gt;"/>        <!--|=f>-->
    <add key="FormatSplitMark" value="&lt;=&gt;"/>    <!--<=>-->
    <add key="CalculationStartMark" value="&lt;c=|"/> <!--<c=|-->
    <add key="CalculationEndMark" value="|=c&gt;"/>   <!--|=c>-->
  </default_setting>
</app_setting>

